{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.org/schemas/css-specifications-index.schema.json",
  "title": "W3C CSS Specifications Index",
  "type": "object",
  "required": [
    "collection_id",
    "name",
    "updated",
    "overview_url",
    "publisher",
    "status",
    "machine_readable_sources",
    "full_spec_inventory",
    "accessibility_relevant_modules"
  ],
  "properties": {
    "collection_id": {"type": "string", "minLength": 1},
    "name": {"type": "string", "minLength": 1},
    "updated": {"type": "string", "minLength": 1},
    "overview_url": {"type": "string", "minLength": 1},
    "publisher": {"type": "string", "minLength": 1},
    "status": {"type": "string", "minLength": 1},
    "machine_readable_sources": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["format", "url"],
        "properties": {
          "format": {"type": "string", "minLength": 1},
          "url": {"type": "string", "minLength": 1}
        },
        "additionalProperties": true
      }
    },
    "full_spec_inventory_source": {"type": "string", "minLength": 1},
    "full_spec_inventory_extracted_at": {"type": "string", "minLength": 1},
    "full_spec_inventory": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["id", "title", "url"],
        "properties": {
          "id": {"type": "string", "minLength": 1},
          "title": {"type": "string", "minLength": 1},
          "url": {"type": "string", "minLength": 1}
        },
        "additionalProperties": true
      }
    },
    "accessibility_relevant_modules": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["id", "title", "url", "relevance"],
        "properties": {
          "id": {"type": "string", "minLength": 1},
          "title": {"type": "string", "minLength": 1},
          "url": {"type": "string", "minLength": 1},
          "relevance": {"type": "string", "minLength": 1}
        },
        "additionalProperties": true
      }
    },
    "related_accessibility_guidance": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "url"],
        "properties": {
          "id": {"type": "string", "minLength": 1},
          "url": {"type": "string", "minLength": 1}
        },
        "additionalProperties": true
      }
    }
  },
  "additionalProperties": true
}