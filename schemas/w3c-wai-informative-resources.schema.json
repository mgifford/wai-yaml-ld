{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.org/schemas/w3c-wai-informative-resources.schema.json",
  "title": "W3C WAI Informative Resources Catalog",
  "type": "object",
  "required": ["project", "updated", "scope", "resources"],
  "properties": {
    "project": {"type": "string", "minLength": 1},
    "updated": {"type": "string", "minLength": 1},
    "scope": {"type": "string", "minLength": 1},
    "primary_wcag_version": {"type": "string"},
    "wcag_version_note": {"type": "string"},
    "resources": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["id", "title", "url", "type"],
        "properties": {
          "id": {"type": "string", "minLength": 1},
          "title": {"type": "string", "minLength": 1},
          "url": {"type": "string", "minLength": 1},
          "type": {"type": "string", "minLength": 1},
          "applies_to": {
            "type": "array",
            "items": {"type": "string", "minLength": 1}
          }
        },
        "additionalProperties": true
      }
    }
  },
  "additionalProperties": true
}
