{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.org/schemas/wai-aria-informative.schema.json",
  "title": "WAI-ARIA Informative Catalog",
  "type": "object",
  "required": [
    "standard_id",
    "name",
    "updated",
    "scope",
    "tr_primary",
    "informative_resources"
  ],
  "properties": {
    "standard_id": {"type": "string", "minLength": 1},
    "name": {"type": "string", "minLength": 1},
    "updated": {"type": "string", "minLength": 1},
    "scope": {"type": "string", "minLength": 1},
    "tr_primary": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["id", "title", "url", "status"],
        "properties": {
          "id": {"type": "string", "minLength": 1},
          "title": {"type": "string", "minLength": 1},
          "url": {"type": "string", "minLength": 1},
          "status": {"type": "string", "minLength": 1}
        },
        "additionalProperties": true
      }
    },
    "informative_resources": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["id", "title", "url", "type"],
        "properties": {
          "id": {"type": "string", "minLength": 1},
          "title": {"type": "string", "minLength": 1},
          "url": {"type": "string", "minLength": 1},
          "type": {"type": "string", "minLength": 1}
        },
        "additionalProperties": true
      }
    }
  },
  "additionalProperties": true
}
