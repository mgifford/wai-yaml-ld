{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.org/schemas/html-living-standard-accessibility.schema.json",
  "title": "HTML Living Standard Accessibility Index",
  "type": "object",
  "required": [
    "standard_id",
    "name",
    "updated",
    "canonical_url",
    "publisher",
    "status",
    "machine_readable_sources",
    "full_section_inventory",
    "accessibility_related_sections"
  ],
  "properties": {
    "standard_id": {"type": "string", "minLength": 1},
    "name": {"type": "string", "minLength": 1},
    "updated": {"type": "string", "minLength": 1},
    "canonical_url": {"type": "string", "minLength": 1},
    "publisher": {"type": "string", "minLength": 1},
    "status": {"type": "string", "minLength": 1},
    "machine_readable_sources": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["format", "url"],
        "properties": {
          "format": {"type": "string", "minLength": 1},
          "url": {"type": "string", "minLength": 1}
        },
        "additionalProperties": true
      }
    },
    "full_section_inventory_source": {"type": "string", "minLength": 1},
    "full_section_inventory_extracted_at": {"type": "string", "minLength": 1},
    "full_section_inventory": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["id", "split_filename", "section_id", "title", "url"],
        "properties": {
          "id": {"type": "string", "minLength": 1},
          "split_filename": {"type": "string", "minLength": 1},
          "section_id": {"type": "string", "minLength": 1},
          "title": {"type": "string", "minLength": 1},
          "url": {"type": "string", "minLength": 1}
        },
        "additionalProperties": true
      }
    },
    "accessibility_related_sections": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["id", "title", "url", "relevance"],
        "properties": {
          "id": {"type": "string", "minLength": 1},
          "title": {"type": "string", "minLength": 1},
          "url": {"type": "string", "minLength": 1},
          "relevance": {"type": "string", "minLength": 1}
        },
        "additionalProperties": true
      }
    },
    "related_accessibility_specs": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "url"],
        "properties": {
          "id": {"type": "string", "minLength": 1},
          "url": {"type": "string", "minLength": 1}
        },
        "additionalProperties": true
      }
    }
  },
  "additionalProperties": true
}